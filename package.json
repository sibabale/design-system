{
  "name": "ozow-ui",
  "version": "0.4.0",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "polished": "4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.8.0",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.6",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "auto shipit --base-branch=main",
    "storybook": "start-storybook -p 6006 -s public",
    "chromatic": "npx chromatic --project-token=${process.env.CHROMATIC_PROJECT_TOKEN}",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook  --docs"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/jest": "0.0.10",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/node-logger": "6.5.16",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.16",
    "@storybook/test-runner": "0.9.4",
    "@storybook/testing-library": "0.0.13",
    "@storybook/theming": "6.5.16",
    "auto": "10.37.6",
    "babel-plugin-named-exports-order": "0.0.2",
    "chromatic": "6.15.0",
    "cross-env": "7.0.3",
    "jest": "27",
    "prettier": "2.8.3",
    "prop-types": "15.8.1",
    "webpack": "5.75.0"
  },
  "readme": "ERROR: No README data found!",
  "description": "A reusable UI library for Ozow",
  "main": "dist/index",
  "repository": "https://github.com/sibabale/ozow-ui.git",
  "author": "Sibabale Joja <sibabalejoja@gmail.com>",
  "license": "MIT"
}
